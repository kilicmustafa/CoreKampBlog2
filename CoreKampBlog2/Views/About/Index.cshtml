@model List<EntityLayer.Concrete.About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@foreach(var item in Model)
{
	<section class="main-content-w3layouts-agileits">
		<div class="container">
			<h3 class="tittle">About</h3>
			<div class="row inner-sec">
				<!--left-->
				<div class="col-lg-8 left-blog-info-w3layouts-agileits text-left">
					<div class="blog-grid-top">

						<div class="blog_info_left_grid">
							<a href="single.html">
								<img src="~/web/images/b5.jpg" class="img-fluid" alt="">
							</a>
						</div>
						<h3>
							@item.AboutDetails1
						</h3>
						<p>@item.AboutDetails2</p>
						<a href="single.html" class="btn btn-primary read-m">Go To Blogs</a>
					</div>
					
				</div>
				<!--//left-->
				<!--right-->
				<aside class="col-lg-4 agileits-w3ls-right-blog-con text-right">
					<div class="right-blog-info text-left">
						@await Component.InvokeAsync("AddNewsLetter")
						@await Component.InvokeAsync("SocialMedia")
						
						@await Component.InvokeAsync("OurProgress")
						@await Component.InvokeAsync("LastThreeBlog")
					</div>

				</aside>
				<!--//right-->
			</div>
		</div>
	</section>

}


